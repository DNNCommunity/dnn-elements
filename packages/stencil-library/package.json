    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-storybook": "^0.6.14",
    "gh-pages": "^6.0.0",
    "jest": "^27.0.3",
    "jest-cli": "^27.5.1",
    "license-checker-rseidelsohn": "^4.2.6",
