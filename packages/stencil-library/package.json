    "axe-playwright": "^2.0.1",
    "babel-loader": "^9.1.2",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-storybook": "^0.11.1",
    "gh-pages": "^6.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "license-checker-rseidelsohn": "^4.2.6",
