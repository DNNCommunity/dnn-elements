{
  "name": "@dnncommunity/dnn-elements",
  "version": "0.12.1-beta.1",
  "description": "Stencil Component Starter",
  "repository": "https://github.com/dnncommunity/dnn-elements",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/dnn/dnn.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "eslint --ext .ts --ext .tsx --ext .js ./src && stencil build --docs",
    "postbuild": "license-checker --out licenses.txt --direct",
    "start": "stencil build --dev --watch --serve",
    "start.edge": "stencil build --dev --watch --serve --es5",
    "poststart": "license-checker --out licenses.txt --direct",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "eslint": "eslint --ext .ts --ext .tsx --ext .js ./src"
  },
  "devDependencies": {
    "@stencil/core": "^2.9.0",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/sass": "^1.5.2",
    "@types/jest": "27.4.0",
    "@types/puppeteer": "5.4.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^27.4.5",
    "jest-cli": "^26.6.3",
    "license-checker": "^25.0.1",
    "puppeteer": "^13.0.1",
    "typescript": "^4.5.5",
    "typescript-debounce-decorator": "^0.0.18"
  },
  "license": "MIT",
  "dependencies": {}
}
