{
  "name": "@eraware/dnn-elements",
  "version": "0.9.1-beta.1",
  "description": "Stencil Component Starter",
  "repository": "https://github.com/eraware/dnn-elements",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/dnn/dnn.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "eslint --ext .ts --ext .tsx --ext .js ./src && stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "start.edge": "stencil build --dev --watch --serve --es5",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "eslint": "eslint --ext .ts --ext .tsx --ext .js ./src"
  },
  "devDependencies": {
    "@stencil/core": "^2.0.3",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.3.2",
    "@types/jest": "26.0.23",
    "@types/puppeteer": "3.0.2",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "26.5.2",
    "jest-cli": "26.6.3",
    "puppeteer": "5.3.1",
    "typescript": "^3.4.3",
    "typescript-debounce-decorator": "^0.0.18"
  },
  "license": "MIT"
}
